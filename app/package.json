    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "5.3.0",
    "html-to-image": "^1.11.11",
    "vue": "^3.2.47",
    "vuetify": "^3.0.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
