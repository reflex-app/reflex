{
  "name": "@reflex/app",
  "productName": "Reflex",
  "description": "Make responsive websites without the guesswork.",
  "version": "0.7.0-beta.32",
  "stableVersion": "0.7.0-beta.31",
  "author": "Nick Wittwer",
  "main": "dist-electron/electron/main.js",
  "homepage": "https://reflexapp.nickwittwer.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/reflex-app/reflex.git"
  },
  "private": true,
  "scripts": {
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "build": "npx cross-env RELEASE=true nuxt generate && yarn run pack",
    "release": "npx @digitak/esrun --tsconfig=./scripts/tsconfig.json ./scripts/updater-cli.ts",
    "pack": "electron-builder --config builder.config.js",
    "screenshot": "yarn run dev & npx wait-on tcp:3000 && node ./scripts/app-screenshots.js",
    "lint": "eslint .",
    "preview": "nuxt preview",
    "lint:fix": "eslint . --fix",
    "test": "npx playwright test",
    "test:ui": "npx playwright test --ui"
  },
  "dependencies": {
    "@electron/remote": "2.0.9",
    "@reflex/browser-installer": "workspace:*",
    "@trpc/client": "^10.4.3",
