{
  "name": "@reflex/app",
  "productName": "Reflex",
  "description": "Make responsive websites without the guesswork.",
  "version": "0.7.0-beta.26",
  "author": "Nick Wittwer",
  "main": "./dist/main/index.js",
  "homepage": "https://reflexapp.nickwittwer.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/reflex-app/reflex.git"
  },
  "scripts": {
    "dev": "node .electron-nuxt/dev.js",
    "build": "npx cross-env RELEASE=true node .electron-nuxt/build.js",
    "build:fast": "node .electron-nuxt/build.js",
    "release": "npx ts-node --logError scripts/updater-cli",
    "test": "jest",
    "lint": "eslint --ext .js,.vue,.ts -f eslint-friendly-formatter ./src",
    "lint:fix": "yarn run lint -- --fix",
    "preinstall": "node .electron-nuxt/check-engines.js",
    "postinstall": "electron-builder install-app-deps",
    "storybook": "start-storybook",
    "debug:playwright": "npx cross-env DEBUG=pw:browser* yarn run dev",
    "debug:build:mac": "./build/mac/Reflex.app/Contents/MacOS/Reflex --args --remote-debugging-port=8315 & open -a \"Google Chrome\" http://localhost:8315",
    "debug:build:m1": "./build/mac-arm64/Reflex.app/Contents/MacOS/Reflex --args --remote-debugging-port=8315 & open -a \"Google Chrome\" http://localhost:8315",
    "screenshot": "yarn run dev & npx wait-on tcp:9080 && node ./scripts/app-screenshots.js"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/renderer/$1",
      "^~/(.*)$": "<rootDir>/src/renderer/$1"
    }
  },
  "dependencies": {
    "playwright-core": "^1.20.1"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.14.8",
    "@electron/remote": "^2.0.8",
    "@nuxt/types": "^2.16.0",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/composition-api": "^0.33.1",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@panzoom/panzoom": "^4.4.3",
    "@pinia-plugin-persistedstate/nuxt": "^1.0.0",
    "@pinia/nuxt": "0.2.1",
    "@reflex/browser-installer": "^1.0.0",
    "@reflex/ui": "^1.0.0",
    "@types/lodash": "^4.14.191",
    "@viselect/vanilla": "^3.0.0-beta.13",
    "@vue/composition-api": "^1.7.1",
    "@vue/test-utils": "^1.2.2",
    "@vueuse/core": "^9.13.0",
    "@xpda-dev/core": "^0.1.6",
    "@xpda-dev/electron-builder": "^0.2.1",
    "@xpda-dev/electron-launcher": "^0.3.0",
    "@xpda-dev/webpack-step": "^0.1.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-vue": "^2.0.2",
    "browser-env": "3.3.0",
    "comlink": "^4.3.1",
    "comlink-loader": "^2.0.0",
    "core-js": "^3.16.0",
    "css-loader": "^5.2.7",
    "deepmerge": "4.2.2",
    "devtron": "^1.4.0",
    "dotenv": "^16.0.0",
    "electron": "^18.3.7",
    "electron-builder": "23.0.2",
    "electron-debug": "3.2.0",
    "electron-devtools-installer": "3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-notarize": "1.2.1",
    "electron-store": "^8.0.2",
    "electron-updater": "5.0.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "html-to-image": "^1.9.0",
    "inquirer": "^8.1.2",
    "is-electron": "^2.2.1",
    "jest": "27.0.6",
    "jest-vue-preprocessor": "^1.7.1",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "mitt": "^3.0.0",
    "moment": "^2.29.4",
    "native-ext-loader": "2.3.0",
    "nuxt": "^2.16.1",
    "pinia": "^2.0.16",
    "pinia-plugin-persistedstate-2": "^2.0.13",
    "playwright": "^1.20.1",
    "postcss": "8.2.10",
    "postcss-import": "13.0.0",
    "postcss-loader": "4.2.0",
    "postcss-url": "10.1.3",
    "prettier": "^2.5.1",
    "sass": "^1.49.10",
    "sass-loader": "10.2.0",
    "shelljs": "^0.8.5",
    "uuid": "8.3.2",
    "vue": "2.7.14",
    "vue-devtools": "5.1.4",
    "vue-feature-flipping": "^3.0.0",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.8",
    "vue-server-renderer": "2.7",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2",
    "vuex-persist": "^3.1.3",
    "webpack": "^4.46.0",
    "webpack-node-externals": "^2.5.2"
  },
  "stableVersion": "0.7.0-beta.25"
}
