{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "This is a sample",
  "repository": {
    "type": "git",
    "url": "https://github.com/reflex-app/reflex.git"
  },
  "main": "./dist/main/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p ./tsconfig.json --noEmitOnError",
    "watch": "nodemon --exec yarn serve:electron",
    "serve:electron": "yarn build && wait-on tcp:3000 && electron .",
    "electron:build": "yarn build",
    "electron:builder": "electron-builder --config builder.config.js",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@reflex/browser-installer": "workspace:^",
    "concurrently": "^7.6.0",
    "electron": "18.3.15",
    "electron-builder": "23.0.2",
    "electron-debug": "3.2.0",
    "electron-devtools-installer": "3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-notarize": "1.2.2",
    "electron-store": "^8.1.0",
    "electron-updater": "5.3.0",
    "json": "^11.0.0",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "mitt": "^3.0.0",
    "moment": "^2.29.4",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4",
    "vue-devtools": "5.1.4",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "playwright-core": "^1.20.1"
  }
}
